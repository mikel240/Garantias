<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atmosfera.garantias">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MisGarantiasActivity" />
        <activity android:name=".fragments.ListadoFragment" />
        <activity android:name=".fragments.CalendarioFragment" />
        <activity android:name=".fragments.ProveedoresFragment" />
        <activity android:name=".activities.AddGarantiaActivity"></activity>
    </application>

</manifest>