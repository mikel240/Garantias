<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atmosfera.garantias">

    <uses-feature android:name="android.hardware.camera2" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.activities.MisGarantiasActivity" />
        <activity android:name=".controllers.fragments.ListadoFragment" />
        <activity android:name=".controllers.fragments.CalendarioFragment" />
        <activity android:name=".controllers.fragments.ProveedoresFragment" />
        <activity android:name=".controllers.activities.AddGarantiaActivity" />
        <activity android:name=".controllers.activities.test" />
        <activity android:name=".controllers.activities.CameraActivity"></activity>
    </application>

</manifest>